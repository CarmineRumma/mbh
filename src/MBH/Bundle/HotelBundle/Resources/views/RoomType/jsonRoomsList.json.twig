{
    "draw" : "{{ draw }}",
    "recordsTotal": "{{ total }}",
    "recordsFiltered": "{{ total }}",
    "data": [
        {% for entity in entities %}
            [
                "<div class='text-center'><i class='fa fa-home'></i></div>",
                "<a href='{{ path('room_type_room_edit', { 'id': entity.id }) }}' rel='main' title='Редактирование записи' data-toggle='tooltip'>{{ entity.name }}</a>",
                "{{ app.request.query.get('search').value }}"
            ]{% if loop.index != entities.count(true) %},{% endif %}
        {% endfor %}
    ]
}