    <![endif]-->

    <script src="http://zamkadom24.sites.maxibooking.ru/assets/js/jquery-1.11.3.min.js"></script>
</head>
<body>

<div id="navbar-top" class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">


        <div class="row">
            <div class="col-xs-3">
                <div class="navbar-header">
                    <div class="navbar-brand"><a href="{{ path('simple_search_index') }}"><img
                                    src="http://zamkadom24.sites.maxibooking.ru/logo.png"></a></div>
                </div>
            </div>
            <div class="col-xs-9">
                <div class="mbh-contact-wrapper">
                    <div class="row">
                        <div class="col-xs-4">
                            <div class="d">для регионов (звонок бесплатный)</div>
                            <div class="t"><i class="fa fa-phone"></i> 8 (800) 555-10-20</div>
                        </div>
                        <div class="col-xs-4">
                            <div class="d">для Москвы</div>
                            <div class="t"><i class="fa fa-phone"></i> 8 (495) 555-10-20</div>
                        </div>
                        <div class="col-xs-4">
                            <a class="btn btn-success btn-sm pull-right btn-auth"
                               href="http://zamkadom24.maxibooking.ru">Вход для клиентов</a>
                            <button data-target="#navbar-main" data-toggle="collapse" type="button"
                                    class="navbar-toggle">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12">

                        <div id="navbar-main" class="navbar-collapse collapse">
                            <ul class="nav navbar-nav">
                                <li class="dropdown">
                                    <a id="themes" href="#" data-toggle="dropdown" class="dropdown-toggle">Themes <span
                                                class="caret"></span></a>
                                    <ul aria-labelledby="themes" class="dropdown-menu">
                                        <li><a href="../default/">Default</a></li>
                                        <li class="divider"></li>
                                        <li><a href="../cerulean/">Cerulean</a></li>
                                        <li><a href="../cosmo/">Cosmo</a></li>
                                        <li><a href="../cyborg/">Cyborg</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a href="../help/">Help</a>
                                </li>
                                <li>
                                    <a href="http://news.bootswatch.com">Blog</a>
                                </li>
                                <li class="dropdown">
                                    <a id="download" href="#" data-toggle="dropdown" class="dropdown-toggle"
                                       aria-expanded="false">Paper <span class="caret"></span></a>
                                    <ul aria-labelledby="download" class="dropdown-menu">
                                        <li><a href="http://jsfiddle.net/bootswatch/ndax7sh7/">Open Sandbox</a></li>
                                        <li class="divider"></li>
                                        <li><a href="./bootstrap.min.css">bootstrap.min.css</a></li>
                                        <li><a href="./bootstrap.css">bootstrap.css</a></li>
                                        <li class="divider"></li>
                                        <li><a href="./variables.less">variables.less</a></li>
                                    </ul>
                                </li>
                            </ul>
                            <ul class="nav navbar-nav navbar-right" style="padding-right:15px;">
                                <li><a class="" href="http://builtwithbootstrap.com/">Контакты</a></li>
                                <li></li>
                            </ul>

                        </div>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>

<div class="mbh-title-wrapper">
    <div class="container">
        <div class="row">

            <div class="col-xs-6">
                <h3>Бронирование отелей</h3>
            </div>
            <div class="col-xs-6">
                <ul class="breadcrumb pull-right">
                    <li><a href="/">Начало</a></li>
                    <li>Бронирование отелей</li>
                </ul>
            </div>

        </div>
    </div>
</div>

<div class="mbh-main-wrapper container">

    <div class="row">

        <div class="col-xs-3">


            <div id="mbh-wigdet-map" class="panel panel-warning">
                <div class="panel-heading">
                    <h3 class="panel-title">Бронирование номеров</h3>
                </div>
                <div class="panel-body">
                    <div id="mbh-form-wrapper" style="overflow: hidden; height: auto;">
                        {{ form|raw }}
                    </div>
                </div>
            </div>

            <div id="mbh-wigdet-map" class="panel panel-info">
                <div class="panel-heading">
                    <h3 class="panel-title">Все объекты на карте</h3>
                </div>
                <div class="panel-body">
                    <div class="mbh-wigdet-map">
                        <a href="{{ path('simple_search_map') }}" class="btn btn-primary" type="submit">Смотреть</a>
                    </div>
                </div>
            </div>


        </div>
        <div class="col-xs-9">
            {% block content %}
            {% endblock content %}
        </div>

    </div>

</div>

<div class="container">

    <footer>
        <div class="row">
            <div class="col-lg-12">
                <p>Based on <a rel="nofollow" href="http://getbootstrap.com">Bootstrap</a>. Icons from <a rel="nofollow"
                                                                                                          href="http://fortawesome.github.io/Font-Awesome/">Font
                        Awesome</a>. Web fonts from <a rel="nofollow" href="http://www.google.com/webfonts">Google</a>.
                </p>

            </div>
        </div>
    </footer>

</div>


<script src="http://zamkadom24.sites.maxibooking.ru/assets/js/bootstrap.min.js"></script>
<script src="http://zamkadom24.sites.maxibooking.ru/assets/plugins/slider_multiple/js/jquery.refineslide.js"></script>
<script src="http://zamkadom24.sites.maxibooking.ru/assets/plugins/slider_multiple/js/wpts_slider_multiple.js"></script>
<link rel="stylesheet"
      href="http://zamkadom24.sites.maxibooking.ru/assets/plugins/slider_multiple/css/slider_multiple.css"/>


<script src="http://zamkadom24.sites.maxibooking.ru/assets/js/scripts.js"></script>

<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/jquery-ui.min.js"></script>
<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/themes/smoothness/jquery-ui.css"/>

<script src="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.0/select2.min.js"></script>
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.0/select2.css"/>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.3.1/jquery.maskedinput.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/typeahead.js/0.11.1/bloodhound.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/typeahead.js/0.11.1/typeahead.jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/typeahead.js/0.11.1/typeahead.bundle.min.js"></script>

