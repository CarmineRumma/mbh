    <style>
        .two-list {
            width: 900px;
            margin-top: 20px;
            border: solid #000000 1px;
        }

        .form {
            float: left;
            border-right: solid #000000 2px;
            width: 448px;
        }

        .form table {
            font-size: 12px;
        }

        .form table .field .title {
            float: left;
            width: 80px;
        }

        .form table td {
            clear: both;
            line-height: 1.3;
        }

        .form table .field .area {
            margin-left: 81px;
        }

        table.main {
            border-collapse: collapse;
        }
        table.main td {
            padding: 1px 0;
        }
