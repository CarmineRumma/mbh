{% extends "MBHBaseBundle::page.html.twig" %}

{% block scripts %}
    {{ parent() }}

    <script src="http://code.highcharts.com/highcharts.js"></script>

    {% javascripts filter='uglifyjs2''@MBHPackageBundle/Resources/public/js/*'%}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% block styles %}
    {{ parent() }}
    
    {% stylesheets filter='cssrewrite, uglifycss''@MBHPackageBundle/Resources/public/css/*'%}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}