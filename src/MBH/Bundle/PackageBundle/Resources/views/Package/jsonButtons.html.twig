{% spaceless %}
    <div class='btn-list text-center'>
        <a href='{{ path('package_edit', { 'id': entity.id }) }}' class='btn btn-success btn-xs' title='Редактирование записи' data-toggle='tooltip'>
            <i class='fa fa-pencil-square-o'></i>
        </a>
        <a href='{{ path('package_delete', { 'id': entity.id}) }}' class='btn btn-danger btn-xs delete-link ' title='Удаление записи' data-toggle='tooltip'>
            <i class='fa fa-trash-o'></i>
        </a>
    </div>
{% endspaceless %}