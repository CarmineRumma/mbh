{% extends "MBHPackageBundle::layout.html.twig" %}

{% set title =  "package.layout.reservation"|trans({}, "MBHPackageBundle")  %}
{% set title_url = path('package') %}

{% block body %}
    {{ parent() }}
    {% include 'MBHPackageBundle:Documents:documentFormModal.html.twig' %}
{% endblock %}

{% if package is not defined and entity is defined and entity.isLocked is defined %}
    {% set package = entity %}
{% endif %}

{% set small_title = small_title ~ ' ' ~ (package is defined and package.isLocked ? ' &nbsp&nbsp<i class="fa fa-lg fa-lock"></i>' : '') %}