                                    {{ weekdays[dynamicSaleDay.dateSales|date('N')]|lower }}
                                </div>
                            </td>
                        {% endif %}
                    {% endfor %}
                    {% if (maxCount - dynamicSale.periods[index-1]|length) > 0 %}
                        {% for i in 1..(maxCount - dynamicSale.periods[index-1]|length) %}
                            <td>-</td>
                        {% endfor %}
