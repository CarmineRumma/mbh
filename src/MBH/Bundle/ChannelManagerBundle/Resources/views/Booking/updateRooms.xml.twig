<?xml version="1.0" encoding="utf-8"?>
<request>
    {% include 'MBHChannelManagerBundle:Booking:credentials.xml.twig' with {'config': config } %}

    {% for roomId, roomInfo in data %}
    <room id="{{ roomId }}">
        {% for date, dateInfo in roomInfo %}
            <date value="{{ date }}">
                <roomstosell>{{ dateInfo.roomstosell }}</roomstosell>
                {% if dateInfo.closed %}
                <closed>1</closed>
                {% endif %}
            </date>
        {% endfor %}
    </room>
    {% endfor %}
</request>