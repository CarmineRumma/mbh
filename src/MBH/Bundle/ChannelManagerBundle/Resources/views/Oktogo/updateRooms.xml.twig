<?xml version="1.0" encoding="utf-8"?>
<request>
    <username>{{ config.hotelId }} </username>
        {% for roomId, roomInfo in data %}
            <room id="{{ roomId }}">
                {% for date, dateInfo in roomInfo %}
                    <update>
                        <date value="{{ date }}"/>
                        <roomstosell>{{ dateInfo.roomstosell }}</roomstosell>
                        {% if dateInfo.closed is defined %}
                            <closed>{{ dateInfo.closed }}</closed>
                        {% endif %}
                    </update>
                {% endfor %}
            </room>
        {% endfor %}
</request>